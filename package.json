{
  "name": "",
  "private": true,
  "scripts": {
    "test": "npm_config_loglevel=silent tap test/** --cov --nyc-arg=--require=babel-register",
    "clean": "rimraf ./coverage && rimraf ./.nyc_output",
    "clean-build": "rimraf ./dist",
    "coverage": "npm run clean && npm run test -- --coverage-report=html",
    "compile": "npm run clean-build && babel ./components --out-dir ./dist/components && babel ./ui-components --out-dir ./dist/ui-components && cp ./app.js ./dist/app.js && cp ./index.html ./dist/index.html && cp ./style.css ./dist/style.css",
    "dev": "nodemon -q -x 'npm test'",
    "repl": "babel-node ./repl.js",
    "env": "env"
  },
  "devDependencies": {
    "acorn": "^3.0.4",
    "babel-cli": "6.5.1",
    "babel-plugin-transform-runtime": "6.5.2",
    "babel-preset-es2015": "6.5.0",
    "babel-register": "6.5.2",
    "babel-runtime": "6.5.0",
    "estraverse": "^4.1.1",
    "node-dir": "^0.1.11",
    "nodemon": "^1.9.1",
    "rimraf": "^2.5.2",
    "tap": "^5.7.0"
  },
  "nyc": {
    "exclude": [
      "test",
      "test{,-*}.js",
      "test-dist",
      "node_modules/**"
    ]
  },
  "dependencies": {
    
  }
}
